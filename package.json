{
  "name": "health-screening-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write",
    "format": "pnpm run fix",
    "format:check": "biome format --check ./src",
    "fix": "biome check --write --unsafe",
    "validate": "pnpm run lint && pnpm run format:check",
    "validate:ci": "biome ci ./src",
    "pre-commit": "pnpm run format && pnpm run validate",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@primeng/themes": "^18.0.0-rc.1",
    "@tailwindcss/postcss": "^4.0.8",
    "@zxing/browser": "^0.1.4",
    "@zxing/library": "^0.20.0",
    "@zxing/ngx-scanner": "^3.9.1",
    "chart.js": "^4.4.2",
    "jwt-decode": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.19",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.0",
    "@biomejs/biome": "2.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "autoprefixer": "^10.4.21",
    "htmlhint": "^1.1.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.5.2"
  }
}
